{% extends 'base.html.twig' %}

{% block title %}Listado profesores{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Profesor</h1>
        
    <ul>
        
            <li>{{ profesor.nombre }} ,{{ profesor.apellido }}, {{profesor.FechaNacimienito|date('d-m-Y')}}</li>
            <p>Especialidad: {{profesor.especialidad.especialidad}}<p>
     
    </ul>
</div>
{% endblock %}
